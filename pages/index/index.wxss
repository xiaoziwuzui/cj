.square-lottery .tags {
    display: flex;
    justify-content: flex-start;
    flex-wrap: nowrap;
    margin-right: 5px;
    max-width: 195px;
}

.square-lottery .tags .tag {
    display: flex;
    flex: 0 1 auto;
    margin: 8px 10px 0 0;
    height: 20px;
    padding: 0 8px;
    background-color: #ffffff;
    color: #575757;
    font-weight: bold;
    box-sizing: border-box;
    align-items: center;
    position: relative;
}

.square-lottery .tags .tag:before {
    border: 1px solid rgba(0,0,0,0.5);
    width: 200%;
    height: 200%;
    position: absolute;
    top: 0;
    left: 0;
    transform: scale(.5);
    pointer-events: none;
    transform-origin: 0 0;
    content: '';
}

.square-lottery .tags .tag:last-child {
    margin-right: 0;
}

.square-lottery .tags .tag text {
    font-size: 11px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.square-lottery .tags .tag image {
    width: 8px;
    height: 8px;
    margin-right: 4px;
    display: flex;
    flex: 0 0 auto;
}

.square-lottery .tags .tag.red {
    border-color: #ad8b8b;
    flex: 0 1 auto;
}

.square-lottery .tags .tag.red:before {
    border: 1px solid #ad8b8b;
    width: 200%;
    height: 200%;
    position: absolute;
    top: 0;
    left: 0;
    transform: scale(.5);
    pointer-events: none;
    transform-origin: 0 0;
    content: '';
}

.square-lottery .tags .tag.red text {
    color: #8f5b5b;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
